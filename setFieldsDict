FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 293
);

regions
(
   boxToCell
	{
        box (0.009 -0.01 -0.01) (0.011 0.01 0.01);
        fieldValues
        (
          volScalarFieldValue T 1800
       );
   }
);


